generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  walletAddress String   @unique
  username      String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  score_credit  Int      @default(650)
}
